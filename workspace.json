{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "test",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "projects": {
    "test": {
      "root": "apps/test",
      "sourceRoot": "apps/test/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/test",
            "index": "apps/test/src/index.html",
            "main": "apps/test/src/main.tsx",
            "polyfills": "apps/test/src/polyfills.ts",
            "tsConfig": "apps/test/tsconfig.app.json",
            "assets": [
              "apps/test/src/favicon.ico",
              "apps/test/src/assets"
            ],
            "styles": [
              "apps/test/src/styles.css"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/test/src/environments/environment.ts",
                  "with": "apps/test/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "test:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "test:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/test/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/test"
          ],
          "options": {
            "jestConfig": "apps/test/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "apps/test/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/test",
            "config": {
              "configFolder": "apps/test/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "test-e2e": {
      "root": "apps/test-e2e",
      "sourceRoot": "apps/test-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/test-e2e/cypress.json",
            "tsConfig": "apps/test-e2e/tsconfig.e2e.json",
            "devServerTarget": "test:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "test:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/test-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "ui-test": {
      "root": "libs/ui-test",
      "sourceRoot": "libs/ui-test/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-test/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/ui-test"
          ],
          "options": {
            "jestConfig": "libs/ui-test/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui-test/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/ui-test",
            "config": {
              "configFolder": "libs/ui-test/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "ui-ang": {
      "projectType": "library",
      "root": "libs/ui-ang",
      "sourceRoot": "libs/ui-ang/src",
      "prefix": "attempt-three",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-ang/src/**/*.ts",
              "libs/ui-ang/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/ui-ang"
          ],
          "options": {
            "jestConfig": "libs/ui-ang/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui-ang/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/ui-ang",
            "config": {
              "configFolder": "libs/ui-ang/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "ui-test-e2e": {
      "root": "apps/ui-test-e2e",
      "sourceRoot": "apps/ui-test-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ui-test-e2e/cypress.json",
            "tsConfig": "apps/ui-test-e2e/tsconfig.e2e.json",
            "devServerTarget": "ui-test:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "ui-test:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ui-test-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "ui-ang-e2e": {
      "root": "apps/ui-ang-e2e",
      "sourceRoot": "apps/ui-ang-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ui-ang-e2e/cypress.json",
            "tsConfig": "apps/ui-ang-e2e/tsconfig.e2e.json",
            "devServerTarget": "ui-ang:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "ui-ang:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ui-ang-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  }
}